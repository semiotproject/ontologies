    <!-- http://w3id.org/qudt/vocab/unit/ext#WattHour -->

    <rdf:Description rdf:about="http://w3id.org/qudt/vocab/unit/ext#WattHour">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Watt-Hour</rdfs:label>
        <qudt:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The watt-hour (abbreviated Wh) is a derived unit of energy. If the energy is being transmitted or used at a constant rate (power) over a period of time, the total energy in kilowatt-hours is the product of the power in kilowatts and the time in hours. The kilowatt-hour is commonly used as a billing unit for energy delivered to consumers by electric utilities.</qudt:description>        
		<qudt:quantityKind rdf:resource="http://qudt.org/vocab/quantity#EnergyAndWork"/>
    </rdf:Description>
